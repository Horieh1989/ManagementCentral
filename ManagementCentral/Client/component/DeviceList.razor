
@ExtraCaption
<h3>DeviceList</h3>


<table class="table">
   <thead>
     <tr>
        <th scope="col">Device Id</th>
        <th scope="col">Location</th>
        <th scope="col">Date</th>
        <th scope="col">Type</th>
         <th scope="col">Status</th>
         <th scope="col">view</th>
         <th scope="col">Delet</th>
         <th scope="col">Update</th>
         <th scope="col">Edit</th>
     </tr>
   </thead>
    <tbody>
        @foreach (var device in DeviceLst)
        {
            <tr>

                <td scope="row">@device.DeviceId </td>
                <td scope="row">@device.Location</td>
                <td scope="row">@device.Date </td>
                <td scope="row">@device.DeviceType </td>
                <td scope="row">@device.Status </td>
                <td scope="row"><a href="@($"DeviceView/{device.DeviceId}")" class="btn btn-outline-primary">View </a> </td>@* reference page ro miarim ba id devive age bekhaim *@
                <td scope="row"><a href="@($"DeviceDelet/{device.DeviceId}")" class="btn btn-outline-primary">Delete</a></td>@* reference page ro miarim ba id devive age bekhaim *@
                <td scope="row"><a href="@($"DeviceUpdate/{device.DeviceId}")" class="btn btn-outline-primary">Update</a></td>@* reference page ro miarim ba id devive age bekhaim *@
                <td scope="row"><a href="@($"DeviceAdd/{device.DeviceId}")" class="btn btn-outline-primary">Edit</a></td>@* reference page ro miarim ba id devive age bekhaim *@
            </tr>

        }
    </tbody>

   

</table>